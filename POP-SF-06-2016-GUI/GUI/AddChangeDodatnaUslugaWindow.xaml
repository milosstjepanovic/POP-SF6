                 Name="tbNaziv" Text="{Binding Path=Naziv}"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="142,90,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"
                 Name="tbCena" Text="{Binding Path=Cena}"/>
        <Button Content="Sacuvaj" HorizontalAlignment="Left" Margin="65,164,0,0" VerticalAlignment="Top" Width="75" Name="btnSacuvaj"
                Click="btnSacuvaj_Click"/>
        <Button Content="Odustani" HorizontalAlignment="Left" Margin="187,164,0,0" VerticalAlignment="Top" Width="75" Name="btnIzlaz"
                Click="btnIzlaz_Click"/>

    </Grid>
</Window>
