<Window x:Class="POP_SF_06_2016_GUI.GUI.NamestajWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP_SF_06_2016_GUI.GUI"
        mc:Ignorable="d"
        Title="NamestajWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="265*"/>
            <RowDefinition Height="56*"/>
        </Grid.RowDefinitions>
        <ListBox Grid.Row="0" x:Name="lbNamestaj" HorizontalAlignment="Left" 
                 Height="265" VerticalAlignment="Top" Width="518" Grid.RowSpan="2"/>
        <Button Grid.Row="1" Width="Auto" Height="30" HorizontalAlignment="Left" 
                Name="btnDodajNamestaj" Click="btnDodajNamestaj_Click">Dodaj namestaj</Button>
        <Button Grid.Row="1" Width="Auto" Height="30" HorizontalAlignment="Left" 
                Name="btnIzmeniNamestaj" Click="btnIzmeniNamestaj_Click" Margin="109,13.2,0,12.8">Izmeni namestaj</Button>
        <Button Grid.Row="1" Width="Auto" Height="30" HorizontalAlignment="Left" 
                Name="btnObrisiNamestaj" Click="btnObrisiNamestaj_Click" Margin="227,13.2,0,12.8">Obrisi namestaj</Button>
        <Button Grid.Row="1" Width="48" Height="30" HorizontalAlignment="Right" 
                Name="btnIzlaz" Click="btnIzlaz_Click" Margin="0,13.2,0.4,12.8">Izlaz</Button>
    </Grid>
</Window>
