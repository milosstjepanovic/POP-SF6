<Window x:Class="POP_SF_06_2016_GUI.GUI.KorisnikWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP_SF_06_2016_GUI.GUI"
        mc:Ignorable="d"
        Title="KorisnikWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="269*"/>
            <RowDefinition Height="52*"/>
        </Grid.RowDefinitions>
        <ListBox Grid.Row="0" x:Name="lbKorisnik" HorizontalAlignment="Left" VerticalAlignment="Top" Height="269" Width="518" Grid.RowSpan="2"/>
        <Button Grid.Row="1" Height="30" Width="Auto" HorizontalAlignment="Left" Name="btnDodajKorisnika" 
                Click="btnDodajKorisnika_Click">Dodaj korisnika</Button>
        <Button Grid.Row="1" Height="30" Width="Auto" HorizontalAlignment="Left" Margin="100,12.2,0,9.8" 
                Name="btnIzmeniKorisnika" Click="btnIzmeniKorisnika_Click">Izmeni korisnika</Button>
        <Button Grid.Row="1" Height="30" Width="Auto" HorizontalAlignment="Left" Margin="207,12.2,0,9.8" 
                Name="btnObrisiKorisnika" Click="btnObrisiKorisnika_Click">Obrisi korisnika</Button>
        <Button Grid.Row="1" Height="30" Width="73" HorizontalAlignment="Right" Margin="0,11.2,0.4,10.8" 
                Name="btnIzlaz" Click="btnIzlaz_Click">Izlaz</Button>
    </Grid>
</Window>
